{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"color": {
			"palette": [
				{
					"slug": "background",
					"color": "#ffffff",
					"name": "Background"
				},
				{
					"slug": "foreground",
					"color": "#444444",
					"name": "Foreground"
				},
				{
					"slug": "primary",
					"color": "#8CC541",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#24A8E0",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#9A9A9A",
					"name": "Tertiary"
				},
				{
					"slug": "darkgreen",
					"color": "#568E0D",
					"name": "Dark green"
				},
				{
					"slug": "darkblue",
					"color": "#157CC0",
					"name": "Dark blue"
				},
				{
					"slug": "concrete",
					"color": "#f7f7f7",
					"name": "Concrete"
				},
				{
					"slug": "charcoal",
					"color": "#282828",
					"name": "Charcoal"
				},
				{
					"slug": "tragic",
					"color": "#9600ff",
					"name": "Tragic"
				},
				{
					"slug": "thunder",
					"color": "#241F20",
					"name": "Thunder"
				},
				{
					"slug": "red",
					"color": "#dc3545",
					"name": "red"
				}
			]
		},
		"layout": {
			"contentSize": "1024px",
			"wideSize": "1440px"
		},
		"typography": {
			"fluid": false,
			"fontSizes": [
				{
					"slug": "small",
					"size": "15px",
					"name": "Small"
				},
				{
					"slug": "base",
					"size": "20px",
					"name": "Base"
				},
				{
					"slug": "large",
					"size": "26px",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "32px",
					"name": "X-Large"
				},
				{
					"slug": "xx-large",
					"size": "40px",
					"name": "2X-Large"
				},
				{
					"slug": "xxx-large",
					"size": "54px",
					"name": "3X-Large"
				},
				{
					"slug": "xxxx-large",
					"size": "72px",
					"name": "4X-Large"
				}
			]
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"spacingScale": {
				"operator": "*",
				"increment": 1.5,
				"steps": 7,
				"mediumStep": 1.5,
				"unit": "rem"
			}
		},
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"dimensions": {
			"minHeight": true,
			"height": true
		}
	},
	"styles": {
		"typography": {
			"fontSize": "var(--wp--preset--font-size--base)"
		},
		"elements": {
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--3x-large)"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--x-large)"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--large)"
                }
            },
			"fontSize": "var(--wp--preset--font-size--large)"
        },
		"blocks": {
			"core/button": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--30)",
						"bottom": "var(--wp--preset--spacing--30)",
						"left": "var(--wp--preset--spacing--60)",
						"right": "var(--wp--preset--spacing--60)"
					}
				},
				"variations": {
					"outline": {
						"spacing": {
							"padding": {
								"top": "var(--wp--preset--spacing--30)",
								"bottom": "var(--wp--preset--spacing--30)",
								"left": "var(--wp--preset--spacing--60)",
								"right": "var(--wp--preset--spacing--60)"
							}
						}
					}
				}
			}
		}
	},
	"core/group": {
		"styles": {
			"border": {
				"radius": 20
			}
		}
	}
	
	
}
